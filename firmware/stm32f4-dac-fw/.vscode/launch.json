  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceRoot}",
      "executable": "./build/stm32f4-dac-fw.elf",
      "name": "Debug with OpenOCD",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "windows": {
        "serverpath": "${env:OPENOCD_DIR}\\bin\\openocd.exe",
        "searchDir": [
          "${env:OPENOCD_DIR}\\share\\openocd\\scripts"
        ],
      },
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f4x.cfg"
      ],     
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none"
    },   
    {
      "cwd": "${workspaceRoot}",
      "executable": "./build/stm32f4-dac-fw.elf",
      "name": "Debug with STLINK",
      "request": "launch",
      "type": "cortex-debug",
      "showDevDebugOutput": "none",      
      "servertype": "stlink",
      "osx": {
        "serverpath": "/opt/ST/STM32CubeCLT/STLink-gdb-server/bin/ST-LINK_gdbserver",
        "stm32cubeprogrammer": "/opt/ST/STM32CubeCLT/STM32CubeProgrammer/bin",
        "svdFile": "/opt/ST/STM32CubeCLT/STMicroelectronics_CMSIS_SVD/STM32F411.svd"
      },  
      "windows": {
        "serverpath": "C:/ST/STM32CubeCLT/STLink-gdb-server/bin/ST-LINK_gdbserver.exe",
        "stm32cubeprogrammer": "C:/ST/STM32CubeCLT/STM32CubeProgrammer/bin",
        "svdFile": "C:/ST/STM32CubeCLT/STMicroelectronics_CMSIS_SVD/STM32F411.svd"
      },      
      "runToEntryPoint": "main",        
    },    
  ]
}